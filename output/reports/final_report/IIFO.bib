@article{Erbil_2013,
  title = {Emergency Admissions Due to Swallowed Foreign Bodies in Adults},
  author = {Erbil, B{\"u}lent and Karaca, Mehmet Ali and Aslaner, Mehmet Ali and Ibrahimov, Zaur and Kunt, Mehmet Mahir and Akpinar, Erhan and {\"O}zmen, Mehmet Mahir},
  year = {2013},
