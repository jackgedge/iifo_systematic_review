@article{revellyLactateGlucoseMetabolism2005,
  title = {Lactate and glucose metabolism in severe sepsis and cardiogenic shock},
  author = {Revelly, Jean Pierre and Tappy, Luc and Martinez, Alexandro and Bollmann, Marc and Cayeux, Marie Christine and Berger, Mette M. and Chiol{\'e}ro, Ren{\'e} L.},
  year = {2005},
  journal = {Critical Care Medicine},
  volume = {33},
  number = {10},
  pages = {2235--2240},
  doi = {10.1097/01.CCM.0000181525.99295.8F},
  abstract = {Objective: To evaluate the relative importance of increased lactate production as opposed to decreased utilization in hyperlactatemic patients, as well as their relation to glucose metabolism. Design: Prospective observational study. Setting: Surgical intensive care unit of a university hospital. Patients: Seven patients with severe sepsis or septic shock, seven patients with cardiogenic shock, and seven healthy volunteers. Interventions: 13C-labeled sodium lactate was infused at 10 {$\mu$}mol/kg/min and then at 20 {$\mu$}mol/kg/min over 120 mins each. 2H-labeled glucose was infused throughout. Measurements and Main Results: Baseline arterial lactate was higher in septic (3.2 {\textpm} 2.6) and cardiogenic shock patients (2.8 {\textpm} 0.4) than in healthy volunteers (0.9 {\textpm} 0.20 mmol/L, p {$<$} .05). Lactate clearance, computed using pharmacokinetic calculations, was similar in septic, cardiogenic shock, and controls, respectively: 10.8 {\textpm} 5.4, 9.6 {\textpm} 2.1, and 12.0 {\textpm} 2.6 mL/kg/min. Endogenous lactate production was determined as the initial lactate concentration multiplied by lactate clearance. It was markedly enhanced in the patients (septic 26.2 {\textpm} 10.5; cardiogenic shock 26.6 {\textpm} 5.1) compared with controls (11.2 {\textpm} 2.7 {$\mu$}mol/kg/min, p {$<$} .01). 13C-lactate oxidation (septic 54 {\textpm} 25; cardiogenic shock 43 {\textpm} 16; controls 65 {\textpm} 15\% of a lactate load of 10 {$\mu$}mol/kg/min) and transformation of 13C-lactate into 13C-glucose were not different (respectively, 15 {\textpm} 15, 9 {\textpm} 18, and 10 {\textpm} 7\%). Endogenous glucose production was markedly increased in the patients (septic 14.8 {\textpm} 1.8; cardiogenic shock 15.0 {\textpm} 1.5) compared with controls (7.2 {\textpm} 1.1 {$\mu$}mol/kg/min, p {$<$} .01) and was not influenced by lactate infusion. Conclusions: In patients suffering from septic or cardiogenic shock, hyperlactatemia was mainly related to increased production, whereas lactate clearance was similar to healthy subjects. Increased lactate production was concomitant to hyperglycemia and increased glucose turnover, suggesting that the latter substantially influences lactate metabolism during critical illness. Copyright {\copyright} 2005 by the Society of Critical Care Medicine and Lippincott Williams \& Wilkins.},
  keywords = {Cardiac surgery,Cardiogenic shock,Critical care,Glucose metabolism,Glucose turnover,Lactate metabolism,Lactic acidosis,msc}
}

@article{reyCatchMeIf2015,
  title = {[Catch me if you can: endoscopic remove of a needle from the jejunum].},
  author = {Rey, J. W. and Gosepath, J. and Hoffman, A. and Kiesslich, R. and Manner, H.},
  year = {2015},
  month = aug,
  journal = {Zeitschrift fur Gastroenterologie},
  volume = {53},
  number = {8},
  pages = {794--797},
  address = {Germany},
  issn = {1439-7803 0044-2771},
  doi = {10.1055/s-0035-1553196},
  abstract = {INTRODUCTION: The ingestion of foreign bodies is a frequently observed problem in daily clinical practice. In order to avoid complications such as perforation,  endoscopic removal of potentially penetrating foreign bodies should be attempted  quickly. The use of various endoscopic techniques has been reported for this  purpose. However, extraction of foreign bodies from the mid gastrointestinal  tract has rarely been reported. CASE REPORT: We present the case of a patient who  had swallowed a safety needle which could safely be removed from the jejunum by  means of double-balloon enteroscopy (DBE). The combination of a thin p-type  enteroscope with a thick t-type overtube was used in order to improve the  manoeuvrability of the endoscope. The needle was pulled into the overtube which  served as a protective shield during the retrieval of the endoscope. CONCLUSION:  Our case report describes the potential of removing foreign bodies from the deep  small bowel by pulling them into the overtube of a double-balloon enteroscope. If  the suspicion of foreign body impaction in the small bowel is made, it may be  advisable to primarily choose a balloon enteroscopy system. Through this, quick  and deep insertion can be combined with a safe removal of the foreign body.},
  copyright = {{\copyright}~Georg Thieme Verlag KG Stuttgart {$\cdot$} New York.},
  langid = {german},
  pmid = {26284328},
  keywords = {Aged,Endoscopy Gastrointestinal/*instrumentation/*methods,exclude,Female,Foreign-Body Migration/pathology/*surgery,Humans,Jejunum/*injuries/pathology/*surgery,msc,Needlestick Injuries/pathology/*surgery,Treatment Outcome},
  file = {/Users/jackgedge/Zotero/storage/VFRDKDAW/Rey et al. - 2015 - Catch Me If You Can Endoscopic Remove Of A Needle.pdf}
}

@article{reynoldsPrognosticationPointofcareEchocardiography2020,
  title = {Prognostication with point-of-care echocardiography during cardiac arrest: A systematic review},
  shorttitle = {Prognostication with point-of-care echocardiography during cardiac arrest},
  author = {Reynolds, Joshua C. and Issa, Mahmoud S. and C. Nicholson, Tonia and Drennan, Ian R. and Berg, Katherine M. and O'Neil, Brian J. and Welsford, Michelle and Andersen, Lars W. and B{\"o}ttiger, Bernd W. and Callaway, Clifton W. and Deakin, Charles D. and Donnino, Michael and Hsu, Cindy H. and Morley, Peter T. and Morrison, Laurie J. and Neumar, Robert W. and Nolan, Jerry P. and Paiva, Edison F. and Parr, Michael J. and Sandroni, Claudio and Scholefield, Barney and Soar, Jasmeet and Wang, Tzong-Luen},
  year = {2020},
  month = jul,
  journal = {Resuscitation},
  volume = {152},
  pages = {56--68},
  issn = {03009572},
  doi = {10.1016/j.resuscitation.2020.05.004},
  urldate = {2023-12-11},
  abstract = {Methods: We conducted this review per PRISMA guidelines and registered with PROSPERO (ID pending). We searched Medline, EMBASE, Web of Science, CINAHL, and the Cochrane Library on September 6, 2019. Two investigators screened titles and abstracts, extracted data, and assessed risks of bias using the Quality in Prognosis Studies (QUIPS) template. We estimated prognostic test performance (sensitivity and specificity) and measures of association (odds ratio). Grading of Recommendations Assessment, Development and Evaluation (GRADE) methodology evaluated the certainty of evidence. Results: In total, 15 studies were included. We found wide variation across studies in the definition of `cardiac motion' and timing of sonographic assessment. Most studies were hindered by high risks of bias from prognostic factor measurement, outcome measurement, and lack of adjustment for other prognostic factors. Ultimately, heterogeneity and risk of bias precluded meta-analyses. We tabulated ranges of prognostic test performance and measures of association for 5 different combinations of definitions of `cardiac motion' and sonographic timing, as well as other miscellaneous sonographic findings. Overall certainty of this evidence is very low. Conclusions: The evidence for using point-of-care echocardiography as a prognostic tool for clinical outcomes during cardiac arrest is of very low certainty and is hampered by multiple risks of bias. No sonographic finding had sufficient and/or consistent sensitivity for any clinical outcome to be used as sole criterion to terminate resuscitation.},
  langid = {english},
  keywords = {Cardiac arrest,Cardiopulmonary resuscitation,Echocardiography,msc,Point-of-care ultrasound,Prognostication,Systematic review},
  file = {/Users/jackgedge/Zotero/storage/3RGDATP2/Reynolds et al. - 2020 - Prognostication with point-of-care echocardiograph.pdf;/Users/jackgedge/Zotero/storage/94NK8FX6/10.1016@j.resuscitation.2020.05.004.pdf.pdf}
}

