@article{mandaviaBedsideEchocardiographyEmergency2001,
  title = {Bedside echocardiography by emergency physicians},
  author = {Mandavia, Diku P. and Hoffner, Richard J. and Mahaney, Kevin and Henderson, Sean O.},
