@article{maValueMDCTDiagnosis2013,
  title = {Value of MDCT in diagnosis and management of esophageal sharp or pointed foreign bodies according to level of esophagus.},
  author = {Ma, Jeehyun and Kang, Doo Kyoung and Bae, Jae-Ik and Park, Kyung Joo and Sun, Joo Sung},
  year = {2013},
