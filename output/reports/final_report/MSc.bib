@article{boltonButtonBatteryMagnet2018,
  title = {Button battery and magnet ingestions in the pediatric patient.},
  author = {Bolton, Scott M. and Saker, Martha and Bass, Lee M.},
  year = {2018},
  month = oct,
  journal = {Current opinion in pediatrics},
  volume = {30},
  number = {5},
  pages = {653--659},
