@article{lederGagReflexDysphagia1996,
  title = {Gag reflex and dysphagia.},
  author = {Leder, S. B.},
  year = {1996 Mar-Apr},
  journal = {Head \& neck},
  volume = {18},
  number = {2},
  pages = {138--141},
