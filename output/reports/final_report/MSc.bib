@article{harrisStomachFireRare2016,
  title = {Stomach on fire: A rare cause of a gastric ulcer after cigarette lighter ingestion-a case report},
  author = {Harris, C. and Stemboroski, L.N. and Shuja, A.},
  year = {2016},
  journal = {American Journal of Gastroenterology},
  volume = {111},
  number = {(Harris C.; Stemboroski L.N.; Shuja A.) University of Florida, College of Medicine, Jacksonville, FL, United States},
  pages = {S1327-S1328},
  issn = {1572-0241},
  doi = {10.1038/ajg.2016.394},
  abstract = {Most foreign bodies ingested pass uneventfully through the GI tract, yet certain objects pose a greater risk. We describe a swallowed cigarette lighter causing a gastric ulcer which was successfully removed with a polypectomy snare. This is the first case of its kind to be reported in the US to our knowledge. 49 year old male with a history of schizophrenia and foreign body ingestion (FBI) presented complaining of epigastric pain, associated with vomiting. Exam was unremarkable. Plain abdominal radiographs demonstrated a foreign body resembling a cigarette lighter visualized within the stomach. Emergent EGD showed congestion and friability in the gastric mucosa with a 5mm non-bleeding cratered ulcer in the incisura. A cigarette lighter was found in the gastric fundus and successfully removed endoscopically with polypectomy snare. He was treated with pantoprazole and was scheduled for a follow up endoscopy. FBI occurs commonly, the of which result in benign passage. Endoscopic removal is necessary in 20\% of cases, however two recent studies have shown a much higher rate of intervention in the setting of intentional ingestion. Adults with psychiatric disorders, intoxication and incarcerated individuals present more frequently with FBI. The incidence of lighter ingestion is not well reported. Patients may identify and localize the discomfort however often it may not correlate with the site of impaction. With FBI the patient may be asymptomatic, have vomiting, drooling, wheezing or respiratory distress. It is imperative to evaluate for peritonitis and small bowel obstruction. Diagnosis can be made by X-rays, non-contrast CT scan or direct endoscopy. Recommendations for emergent endoscopic intervention include complete obstruction, batteries and sharp objects but do not comment on cigarette lighters. Harmful substances contained in lighter fluid include benzene and propane. The chemical properties of the individual hydrocarbon determine the specific toxicity, while the dose and route of ingestion affect which organs are exposed to the toxicity. In our case the material lead to direct destruction of the mucosa. Complications from this direct damage may have included perforation, abscess formation, internal fistula and hemorrhage. In conclusion, immediate removal of cigarette lighters and all other foreign bodies containing hazardous material regardless of size and shape should be done. (Figure Presented).},
  langid = {english},
  keywords = {abscess,adult,benzene,case report,complication,computer assisted tomography,dangerous goods,destruction,diagnosis,drug toxicity,endoscopy,epigastric pain,exposure,fistula,follow up,foreign body,human,hypersalivation,ingestion,intoxication,liquid,male,middle aged,msc,organ,pantoprazole,perforation,peritonitis,polypectomy,propane,respiratory distress,schizophrenia,side effect,small intestine obstruction,stomach fundus,stomach mucosa,stomach ulcer,toxicity,vomiting,wheezing,X ray film}
}

