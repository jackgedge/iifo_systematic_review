  langid = {english},
  pmid = {39384638},
  keywords = {*Electric Power Supplies,*Foreign Bodies/surgery/complications,Child,Child Preschool,Clinical Protocols,Complication disk battery ingestion,Disk battery ingestion,Esophageal Fistula/surgery/etiology,Esophagus/surgery,exclude,Female,Humans,Infant,Male,msc,Protocol,Retrospective Studies,Tomography X-Ray Computed,Tracheoesophageal Fistula/surgery},
