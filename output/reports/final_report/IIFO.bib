@article{Nock_2008,
  title = {Cross-National Prevalence and Risk Factors for Suicidal Ideation, Plans and Attempts},
  author = {Nock, Matthew K. and Borges, Guilherme and Bromet, Evelyn J. and Alonso, Jordi and Angermeyer, Matthias and Beautrais, Annette and Bruffaerts, Ronny and Chiu, Wai Tat and de Girolamo, Giovanni and Gluzman, Semyon and de Graaf, Ron and Gureje, Oye and Haro, Josep Maria and Huang, Yueqin and Karam, Elie and Kessler, Ronald C. and Lepine, Jean Pierre and Levinson, Daphna and {Medina-Mora}, Maria Elena and Ono, Yutaka and {Posada-Villa}, Jos{\'e} and Williams, David},
  year = {2008},
