  pmid = {29900648},
  keywords = {*Foreign Bodies,*Self-Injurious Behavior,Adult,Attempted Suicide,Emergency Service Hospital,endoscopy,fav,Female,Foreign Bodies,foreign object ingestion,Humans,IIFO,Ingestion,Intervention,Male,Mental Disorders,Mental Disorders/*psychology,msc,Nonsuicidal Self-Injury,Prisoners,Prisoners/*psychology,Prisons,read,self-harm,Self-Injurious Behavior},
