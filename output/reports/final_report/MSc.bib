@article{yeganeSurgicalApproachBody2009,
  title = {Surgical approach to body packing.},
  author = {Yegane, Rooh-Allah and Bashashati, Mohammad and Hajinasrollah, Esmail and Heidari, Kamran and Salehi, Noor-Allah and Ahmadi, Mina},
