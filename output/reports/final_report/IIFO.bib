  pmid = {30808405},
  keywords = {Acute tricyclic antidepressant poisoning,Adult,Antidepressive Agents Tricyclic/pharmacokinetics/*poisoning,Bezoars/*chemically induced/pathology,Charcoal/therapeutic use,Clomipramine,Clomipramine/pharmacokinetics/*poisoning,Delayed-Action Preparations/pharmacokinetics/*poisoning,Domperidone/pharmacokinetics/*poisoning,Drug overdose,Drug Overdose/complications/*pathology,Endoscopy,Female,Humans,Lorazepam/pharmacokinetics/*poisoning,Pharmacobezoar,Suicide Attempted,Sustained-release formulation,Treatment Outcome}
