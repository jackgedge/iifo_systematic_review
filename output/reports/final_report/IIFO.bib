@article{Aitchison_2024,
  title = {Self-Harm in Immigration Detention: Political, Not (Just) Medical},
  shorttitle = {Self-Harm in Immigration Detention},
  author = {Aitchison, Guy and Essex, Ryan},
  year = {2024},
  month = nov,
  journal = {Journal of Medical Ethics},
  volume = {50},
  number = {11},
  pages = {786--793},
  issn = {0306-6800, 1473-4257},
  doi = {10.1136/jme-2022-108366},
  urldate = {2024-10-29},
  abstract = {Self-\-harm within immigration detention centres has been a widely documented phenomenon, occurring at far higher rates than the wider community. Evidence suggests that factors such as the conditions of detention and uncertainty about refugee status are among the most prominent precipitators of self-\-harm. While important in explaining self-h\- arm, this is not the entire story. In this paper, we argue for a more overtly political interpretation of detainee self-\-harm as resistance and assess the ethical implications of this view, drawing on interviews with detainees from Australia's offshore system. Self-\-harm by detainees is not only a medical 'condition' arising in response to oppression but a form of political action to lessen or contest it. We first establish how self-\-harm could be conceptualised as resistance. We then discuss its political purpose, noting it serves at least three functions: intrinsic, instrumental and disruptive or coercive. Viewing detainee self-\- harm as political resistance is a supplement to (rather than a substitute for) a medical approach. However, conceptualising self-\-harm this way has several advantages, namely, moving away from the idea that such behaviour is 'maladaptive', recognising detainees as political agents, combatting government claims of 'manipulation' and 'blackmail' and clarifying the duties of healthcare workers who work in detention.},
  langid = {english},
  file = {/Users/jackgedge/Zotero/storage/XZ8DIQAJ/Aitchison and Essex - 2024 - Self-harm in immigration detention political, not.pdf}
}

