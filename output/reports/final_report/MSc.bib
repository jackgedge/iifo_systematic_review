  journal = {QJM: An International Journal of Medicine},
  volume = {106},
  number = {8},
  pages = {783--784},
  issn = {1460-2725},
  doi = {10.1093/qjmed/hcs165},
  urldate = {2025-04-14},
  abstract = {A 29-year-old female with a history of psychiatric disorder that was treated with thorazine and lithium, but was not taking any of her medications, was brought to the hospital after being witnessed by her family swallowing razor blades. Her only symptom was abdominal cramping. On examination, her vital signs were stable and she appeared to be in no acute distress. On examination, she had multiple old self-inflicted scars on her arms, wrists and thighs. She admitted that she had swallowed ten razor blades after coating them with chewing gum but she denied any intentions to hurt herself. Vitals were stable and her physical examination was unremarkable. Laboratory work up was within normal with stable hemoglobin and hemarocrit. Acute abdominal X-ray showed razor blades overlying the upper abdomen, likely in the gastric antrum (Figure 1). Four days later, repeated X-rays showed multiple razor blades present in the rectum (Figure 2). Eventually, all razor blades were passed. Psychiatry was consulted for management of schizoaffective schizophrenia and she was restarted on her home medications. She was discharged in a stable condition.},
  keywords = {msc},
  file = {/Users/jackgedge/Zotero/storage/XJCQSEGM/Ataya et al. - 2013 - Razor blades in the stomach.pdf;/Users/jackgedge/Zotero/storage/LST2Z6GY/1575400.html}
}

@article{Atayan_2016,
