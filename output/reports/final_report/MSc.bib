@article{edelmanPOSTTRAUMATICHYPOTENSIONSHOULD2007,
  title = {POST-TRAUMATIC HYPOTENSION: SHOULD SYSTOLIC BLOOD PRESSURE OF 90-109 MMHg BE INCLUDED?},
  shorttitle = {POST-TRAUMATIC HYPOTENSION},
  author = {Edelman, David A. and White, Michael T. and Tyburski, James G. and Wilson, Robert F.},
