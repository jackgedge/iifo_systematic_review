@article{delrossoClinicalPredictorsCardiac2008,
  title = {Clinical predictors of cardiac syncope at initial evaluation in patients referred urgently to a general hospital: the EGSYS score},
  shorttitle = {Clinical predictors of cardiac syncope at initial evaluation in patients referred urgently to a general hospital},
  author = {Del Rosso, A and Ungar, A and Maggi, R and Giada, F and Petix, N R and De Santo, T and Menozzi, C and Brignole, M},
