  author = {Breitkreutz, Raoul and Price, Susanna and Steiger, Holger V. and Seeger, Florian H. and Ilper, Hendrik and Ackermann, Hanns and Rudolph, Marcus and Uddin, Shahana and Weigand, Markus A. and M{\"u}ller, Edgar and Walcher, Felix},
  year = {2010},
