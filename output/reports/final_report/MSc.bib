@article{nencioniMicrocirculationDiagnosticTherapeutic2009,
  title = {The microcirculation as a diagnostic and therapeutic target in sepsis},
  author = {Nencioni, Andrea and Trzeciak, Stephen and Shapiro, Nathan I.},
  year = {2009},
  month = oct,
  journal = {Internal and Emergency Medicine},
  volume = {4},
  number = {5},
  pages = {413--418},
  issn = {1828-0447, 1970-9366},
  doi = {10.1007/s11739-009-0297-5},
  urldate = {2023-12-11},
  abstract = {The microcirculation is defined as the smallest vessels where gas and nutrient exchange with tissues takes place. One of its primary functions is to ensure adequate oxygen delivery to meet the oxygen demands of tissue cells. Previous data from clinical and experimental studies and the recent development of new imaging modalities, such as Orthogonal Polarization Spectral videomicroscopy and Sidestream Dark Field imaging, have helped to identify the crucial role that microcirculation plays in sepsis. If not corrected, microcirculatory dysfunction can lead to respiratory distress in tissue cells and subsequent organ failure, even in the absence of global hemodynamic deficiency. In the present review, we will address past and recent developments regarding the role of the microcirculation as an important target in the pathogenesis of sepsis and its progression to multiple organ failure. Accordingly, we identify the microcirculation as an important diagnostic and therapeutic target for treatment in sepsis.},
  langid = {english},
  keywords = {Endothelial injury,Inflammatory response,Microcirculation,msc,Septic shock,Shock},
  file = {/Users/jackedge/Library/Mobile Documents/com~apple~CloudDocs/Documents/Zotero Annotations/Nencioni et al_2009_The microcirculation as a diagnostic and therapeutic target in sepsis.pdf;/Users/jackgedge/Zotero/storage/3NKKKSBA/nencioni2009.pdf.pdf;/Users/jackgedge/Zotero/storage/4CFMJIEG/nencioni2009.pdf.pdf;/Users/jackgedge/Zotero/storage/M4NTZJME/Nencioni et al. - 2009 - The microcirculation as a diagnostic and therapeut.pdf;/Users/jackgedge/Zotero/storage/R4A9DKVV/nencioni2009.pdf.pdf}
}

@article{nessenFreshWholeBlood2013,
  title = {Fresh whole blood use by forward surgical teams in {\textsc{A}} fghanistan is associated with improved survival compared to component therapy without platelets},
  shorttitle = {Fresh whole blood use by forward surgical teams in},
  author = {Nessen, Shawn C. and Eastridge, Brian J. and Cronk, Daniel and Craig, Robert M. and Bers{\'e}us, Olle and Ellison, Richard and Remick, Kyle and Seery, Jason and Shah, Avani and Spinella, Philip C.},
  year = {2013},
  month = jan,
  journal = {Transfusion},
  volume = {53},
  number = {S1},
  issn = {0041-1132, 1537-2995},
  doi = {10.1111/trf.12044},
  urldate = {2023-12-11},
  abstract = {BACKGROUND: In Afghanistan, a substantial portion of resuscitative combat surgery is performed by US Army forward surgical teams (FSTs). Red blood cells (RBCs) and fresh frozen plasma (FFP) are available at these facilities, but platelets are not. FST personnel frequently encounter high-acuity patient scenarios without the ability to transfuse platelets. An analysis of the use of fresh whole blood (FWB) at FSTs therefore allows for an evaluation of outcomes associated with this practice. STUDY DESIGN AND METHODS: A retrospective analysis was performed in prospectively collected data from all transfused patients at six FSTs from December 2005 to December 2010. Univariate analysis was performed, followed by two separate propensity score analyses. In-hospital mortality was predicted with the use of a conditional logistic regression model that incorporated these propensity scores. Subset analysis included evaluation of patients who received uncrossmatched Type O FWB compared with those who received type-specific FWB. RESULTS: A total of 488 patients received a blood transfusion. There were no significant differences in age, sex, or Glasgow Coma Scale in those who received or did not receive FWB. Injury Severity Scores were higher in patients transfused FWB. In our adjusted analyses, patients who received RBCs and FFP with FWB had improved survival compared with those who received RBCs and FFP without FWB. Of 94 FWB recipients, 46 FWB recipients (49\%) were given uncrossmatched Type O FWB, while 48 recipients (51\%) received type-specific FWB. There was no significant difference in mortality between patients that received uncrossmatched Type O and type-specific FWB. CONCLUSIONS: The use of FWB in austere combat environments appears to be safe and is independently associated with improved survival to discharge when compared with resuscitation with RBCs and FFP alone. Mortality was similar for patients transfused uncrossmatched Type O compared with ABO typespecific FWB in an austere setting.},
  langid = {english},
  keywords = {msc},
  file = {/Users/jackedge/Library/Mobile Documents/com~apple~CloudDocs/Documents/Zotero Annotations/Nessen et al_2013_Fresh whole blood use by forward surgical teams in span style=font-variant.pdf;/Users/jackgedge/Zotero/storage/KA72D4WC/Nessen et al. - 2013 - Fresh whole blood use by forward surgical teams in.pdf;/Users/jackgedge/Zotero/storage/SWKCJKC5/Nessen et al. - 2013 - Fresh whole blood use by forward surgical teams in.pdf;/Users/jackgedge/Zotero/storage/XWGGY9HC/Nessen et al. - 2013 - Fresh whole blood use by forward surgical teams in.pdf;/Users/jackgedge/Zotero/storage/Y9EWHYBD/Nessen et al. - 2013 - Fresh whole blood use by forward surgical teams in.pdf}
}

