@article{ojioCaseSubmucosalAbscess2021,
  title = {A case of submucosal abscess of the esophagus mimicking a mediastinal abscess.},
  author = {Ojio, Hidenori and Tanaka, Yoshihiro and Sato, Yuta and Imai, Takeharu and Okumura, Naoki and Matsuhashi, Nobuhisa and Takahashi, Takao and Yoshida, Kazuhiro},
  year = {2021},
  month = apr,
  journal = {Clinical journal of gastroenterology},
