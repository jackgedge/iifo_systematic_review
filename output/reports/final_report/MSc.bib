  author = {Freund, Yonathan and Viglino, Damien and Cachanado, Marine and Cassard, Cl{\'e}mentine and Montassier, Emmanuel and Douay, B{\'e}nedicte and Guenezan, J{\'e}r{\'e}my and Le Borgne, Pierrick and Yordanov, Youri and Severin, Armelle and Roussel, M{\'e}lanie and Daniel, Matthieu and Marteau, Adrien and Peschanski, Nicolas and Teissandier, Dorian and Macrez, Richard and Morere, Julia and Chouihed, Tahar and Roux, Damien and Adnet, Fr{\'e}d{\'e}ric and Bloom, Ben and Chauvin, Anthony and Simon, Tabassome},
  year = {2023},
