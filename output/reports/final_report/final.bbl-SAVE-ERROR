      \strng{namehash}{ccc77fef9437608ff8560c6c83da9913}
      \strng{fullhash}{ccc77fef9437608ff8560c6c83da9913}
      \strng{fullhashraw}{ccc77fef9437608ff8560c6c83da9913}
      \strng{bibnamehash}{ccc77fef9437608ff8560c6c83da9913}
