@article{jacobDeathSwallowingAspiration1990,
  title = {Death, After Swallowing and Aspiration of a High Number of Foreign Bodies, in a Schizophrenic Woman:},
  shorttitle = {Death, After Swallowing and Aspiration of a High Number of Foreign Bodies, in a Schizophrenic Woman},
  author = {Jacob, Bernhard and Huckenbeck, Wolfgang and Barz, J{\"u}rgen and Bonte, Wolfgang},
