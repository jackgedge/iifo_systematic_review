      \strng{namehash}{381e1434ddfb160fd1ea9be24b78b809}
      \strng{fullhash}{381e1434ddfb160fd1ea9be24b78b809}
      \strng{fullhashraw}{381e1434ddfb160fd1ea9be24b78b809}
      \strng{bibnamehash}{381e1434ddfb160fd1ea9be24b78b809}
