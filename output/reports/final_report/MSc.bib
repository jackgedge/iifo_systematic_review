@article{newmanPulmonaryAspirationComplexes1982,
  title = {Pulmonary aspiration complexes in adults},
  author = {Newman, Glenn E. and Effman, Eric L. and Putman, Charles E.},
  year = {1982},
  month = jul,
  journal = {Current Problems in Diagnostic Radiology},
  volume = {11},
  number = {4},
  pages = {5--47},
  issn = {0363-0188},
  doi = {10.1016/0363-0188(82)90028-7},
  urldate = {2025-01-13},
  abstract = {The term pulmonary aspiration complex represents the various types of pulmonary aspiration based on the nature of the material aspirated. Pulmonary aspiration continues to be an ever-threatening clinical problem even though knowledge of lung defense mechanisms and predisposing factors of aspiration has been advanced to a point which allows identification of patients at risk of suffering pulmonary aspiration. The identification of patients at risk however has not permitted adequate preventive measures from being exercised. This problem persists in part because few of the predisposing factors are reversible or controllable to a degree which would preclude pulmonary aspiration. In addition, the clinical diagnosis of pulmonary aspiration is more often tentative than absolute and based on the clinical setting and changes demonstrated on serial chest radiographs. This situation emphasizes the role of the radiologist and his knowledge of radiographic findings in pulmonary aspiration in suggesting the diagnosis of pulmonary aspiration. The diagnosis may then be confirmed by additional chest radiographs or by bronchoscopy and the retrieval of the aspirated material. An understanding of the pathophysiology of the types of pulmonary aspiration serves to increase the awareness of certain pulmonary complications from aspiration, to select appropriate diagnostic studies, and to institute specific therapy. However, the nature of the material aspirated by a patient may not be obvious initially unless the episode is witnessed and may only be determined in retrospect in some cases. Thus, radiologists play a primary role in the diagnosis of pulmonary aspiration. Frequently, radiologists must integrate the chest radiographic findings of pulmonary aspiration---the distribution, types of parenchymal opacities, and the expected temporal changes in these opacities, with the patient's clinical situation to establish a diagnosis of pulmonary aspiration (Tables 5, 6). Until recently, therapy of pulmonary aspiration was standardized, pragmatic, and frequently lacked an understanding of the distinct types of pulmonary aspiration. Controversy still surrounds some aspects of therapy. Perhaps, with additional efforts.},
  keywords = {exclude,msc},
  file = {/Users/jackgedge/Zotero/storage/VGNGF7XR/Ge Newman - 1982 - Pulmonary Aspiration Complexes In Adults.pdf;/Users/jackgedge/Zotero/storage/5RHGSEE8/0363018882900287.html}
}

