@article{dranovaDifficultOesophagealForeign2024,
  title = {Difficult oesophageal foreign body removal: a novel surgical approach to a complex situation.},
  author = {Dranova, Sabina and Siddiqui, Zohaib and Tobbal, Muhammad and Pitkin, Lisa},
  year = {2024},
  month = jun,
  journal = {The Journal of laryngology and otology},
  volume = {138},
  number = {6},
  pages = {703--706},
  address = {England},
  issn = {1748-5460 0022-2151},
  doi = {10.1017/S0022215124000033},
  abstract = {BACKGROUND: Ingested foreign bodies pose a unique challenge in medical practice, especially when lodged in the oesophagus. While endoscopic retrieval is the  standard treatment, certain cases require more innovative approaches. METHODS:  This paper reports the case of a patient who intentionally ingested a butter  knife that lodged in the thoracic oesophagus. After multiple endoscopic attempts,  a lateral neck oesophagotomy, aided using a Hopkins rod camera and an improvised  trochar as a protective port, was performed. RESULTS: The foreign body was  successfully extracted without causing oesophageal perforation. The patient was  made nil by mouth, with nasogastric feeding only until a swallow assessment after  one week. The patient was discharged and recovered well. CONCLUSION: This case  illustrates a successful, innovative approach to removing a foreign body in a  high-risk patient, highlighting the significance of adaptability in surgical  practice. It emphasises the need for individualised approaches based on the  patient's history, the nature and location of the foreign body, and associated  risks.},
  langid = {english},
  pmid = {38224046},
  keywords = {*Esophagoscopy/methods,*Esophagus/surgery,*Foreign Bodies/surgery,endoscopy,esophagus,Foreign bodies,Humans,Male,msc},
  file = {/Users/jackgedge/Zotero/storage/5VW5YHQJ/Dranova et al. - 2024 - Difficult Oesophageal Foreign Body Removal A Nove.pdf}
}

