      \strng{namehash}{3ef6a67e7f10a8f04beb5b3584dcab40}
      \strng{fullhash}{3ef6a67e7f10a8f04beb5b3584dcab40}
      \strng{fullhashraw}{3ef6a67e7f10a8f04beb5b3584dcab40}
