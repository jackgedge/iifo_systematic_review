@article{karimanValidationEGSYSScore2015,
  title = {Validation of EGSYS Score in Prediction of Cardiogenic Syncope},
  author = {Kariman, Hamid and Harati, Sepideh and Safari, Saeed and Baratloo, Alireza and Pishgahi, Mehdi},
  year = {2015},
  journal = {Emergency Medicine International},
