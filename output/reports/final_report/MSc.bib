  keywords = {Adolescent,Adult,Aged,Aged 80 and over,Deglutition,Emergency,Emergency Service Hospital,Endoscopy,exclude,Female,Fiber Optic Technology,Foreign Bodies,Foreign body,Gastrointestinal tract,Humans,Ingestion,Male,Middle Aged,msc,Patient Admission,Predictive Value of Tests,Prognosis,Radiography,Retrospective Studies,Young Adult},
