  langid = {english},
  pmid = {34191051},
  keywords = {*Foreign Bodies/epidemiology/surgery,Battery ingestion,Button batteries,Child,Children,Cylindrical batteries,Eating,Electric Power Supplies,Esophagus,exclude,Humans,msc,Retrospective Studies,Stomach},
