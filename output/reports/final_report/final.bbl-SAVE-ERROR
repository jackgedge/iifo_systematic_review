      \strng{fullhashraw}{e9f34e035b98fbe2e11db34670c382f0}
      \strng{bibnamehash}{e9f34e035b98fbe2e11db34670c382f0}
      \strng{authorbibnamehash}{e9f34e035b98fbe2e11db34670c382f0}
      \strng{authornamehash}{e9f34e035b98fbe2e11db34670c382f0}
