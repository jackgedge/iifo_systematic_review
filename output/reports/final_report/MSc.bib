  issn = {1863-4362},
  doi = {10.1007/s11845-007-0080-4},
  abstract = {BACKGROUND: A 29-year-old man was admitted with a 7-day history of progressive non-specific abdominal pain that progressed to small bowel obstruction following ingestion of a 20-pence coin 4-months previously. Colonoscopic retrieval was unsuccessful. A subsequent laparotomy revealed a chronically inflamed thickened terminal ileum with mesenteric fat encroachment necessitating a right hemicolectomy. Histopathological analysis confirmed Crohn's disease with impaction of the 20-pence coin in a distal terminal ileum stricture near the ileo-caecal valve. LEARNING POINT: Gastrointestinal foreign body retention should alert the clinician to the presence of an undiagnosed bowel abnormality. Furthermore, failed endoscopic retrieval should be considered as a marker for potential underlying gastrointestinal pathology and a requirement for operative intervention. CONCLUSION: This case describes a rare presentation of Crohn's disease and highlights the need to consider underlying gastrointestinal pathology in patients presenting with a deteriorating clinical condition in the presence of an incidental foreign body.},
  langid = {english},
  pmid = {17705042},
  keywords = {Abdominal Pain,Adult,Crohn Disease,exclude,Foreign Bodies,Humans,Ileum,Intestinal Obstruction,Laparotomy,Male,msc},
  file = {/Users/jackgedge/Zotero/storage/42HBSTHB/O'Donnell et al. - 2008 - Crohn's disease of the terminal ileum a cheap dia.pdf}
}

