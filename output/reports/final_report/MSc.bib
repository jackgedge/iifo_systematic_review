@article{assavapokeeDecodingVExUSPractical2024,
  title = {Decoding VExUS: a practical guide for excelling in point-of-care ultrasound assessment of venous congestion},
  shorttitle = {Decoding VExUS},
  author = {Assavapokee, Taweevat and Rola, Philippe and Assavapokee, Nicha and Koratala, Abhilash},
  year = {2024},
