    \entry{Aitchison_2024}{article}{}{}
      \name{author}{2}{}{%
        {{hash=a1bc1a30fd7c25cd42d196dd48298763}{%
           family={Aitchison},
           familyi={A\bibinitperiod},
           given={Guy},
