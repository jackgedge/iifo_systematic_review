  shorttitle = {Diagnosing Acute Heart Failure in Patients With Undifferentiated Dyspnea},
  author = {Russell, Frances M. and Ehrman, Robert R. and Cosby, Karen and Ansari, Asim and Tseeng, Stephanie and Christain, Errick and Bailitz, John},
