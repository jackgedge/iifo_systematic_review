  pmid = {30539244},
  keywords = {*Bone and Bones,*Upper Gastrointestinal Tract,Adult,Aged,Animals,Chi-Square Distribution,Complication,exclude,Female,Fish bone,Follow-Up Studies,Foreign-Body Migration/*complications/diagnosis,Humans,Logistic Models,Male,Middle Aged,msc,Multivariate Analysis,Penetration,Retrospective Studies,Risk factor,Risk Factors,Seafood/*adverse effects,Upper gastrointestinal tract},
