@article{khanPulmonaryVeinIsolationAtrial2008,
  title = {Pulmonary-Vein Isolation for Atrial Fibrillation in Patients with Heart Failure},
  author = {Khan, Mohammed N. and Ja{\"i}s, Pierre and Cummings, Jennifer and Di Biase, Luigi and Sanders, Prashanthan and Martin, David O. and Kautzner, Josef and Hao, Steven and Themistoclakis, Sakis and Fanelli, Raffaele and Potenza, Domenico and Massaro, Raimondo and Wazni, Oussama and Schweikert, Robert and Saliba, Walid and Wang, Paul and {Al-Ahmad}, Amin and Beheiry, Salwa and Santarelli, Pietro and Starling, Randall C. and Russo, Antonio Dello and Pelargonio, Gemma and Brachmann, Johannes and Schibgilla, Volker and Bonso, Aldo and Casella, Michela and Raviele, Antonio and Ha{\"i}ssaguerre, Michel and Natale, Andrea},
  year = {2008},
