@article{geraciRetrospectiveAnalysisManagement2016,
  title = {Retrospective analysis of management of ingested foreign bodies and food impactions in emergency endoscopic setting in adults},
  author = {Geraci, Girolamo and Sciume', Carmelo and Di Carlo, Giovanni and Picciurro, Antonino and Modica, Giuseppe},
  year = {2016},
