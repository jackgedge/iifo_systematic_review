  pmid = {8650970},
  keywords = {Adolescent,Adult,Aged,Ambulatory Care,Digestive System,exclude,Female,Follow-Up Studies,Foreign Bodies,Gastrointestinal Transit,Humans,Male,Middle Aged,msc,Radiography,Treatment Outcome}
