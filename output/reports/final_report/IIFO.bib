@article{Cevik_2013,
  title = {The Characteristics and Outcomes of Foreign Body Ingestion and Aspiration in Children Due to Lodged Foreign Body in the Aerodigestive Tract},
  author = {Cevik, Muazez and G{\'o}kdemir, Mehmet Tahir and Boleken, Mehmet Emin and Sogut, Ozgur and Kurkcuoglu, Can},
