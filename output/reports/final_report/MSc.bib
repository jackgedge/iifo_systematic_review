  keywords = {alcoholism,Alcoholism,Alcoholism/*psychology,diagnosis,exclude,foreign bodies,Foreign Bodies,Foreign Bodies/*diagnostic imaging/*surgery,Humans,Lung,Lung/diagnostic imaging,Male,Middle Aged,msc,Paranoid Schizophrenia,personality disorders,Personality Disorders,Psychodiagnosis,Radiography,recurrence,Relapse (Disorders),Respiratory Aspiration,Respiratory Aspiration/diagnosis/*psychology,schizophrenia,Schizophrenia Paranoid,Schizophrenia Paranoid/*psychology},
