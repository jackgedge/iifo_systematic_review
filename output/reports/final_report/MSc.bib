@article{ardilaUtilizationSingleIncisionLaparoscopy2024,
  title = {Utilization of Single-Incision Laparoscopy in the Management of Ingested Magnets.},
  author = {Ardila, Sara and Woodley, Lucille and Ulloa, Emily and Fernandez, Jenelle and Bornstein, Jeffery and Seims, Aaron},
  year = {2024},
