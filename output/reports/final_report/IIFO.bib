@article{Dalma-Weiszhausz_2021,
  title = {Multiple {{Penetrating Eye Wounds Due}} to {{Suspected Self-Injury}}.},
  author = {{Dalma-Weiszhausz}, Jos{\'e} and Alvarado, Jos{\'e} Arturo Oyervides and Licona, Ana Maria Suarez and Azuara, Miriam Tatiana Serment and Kende, Alejandro Dalma},
