      \strng{fullhashraw}{e816d9e7d0b669372ae812b975328a79}
      \strng{bibnamehash}{e816d9e7d0b669372ae812b975328a79}
      \strng{authorbibnamehash}{e816d9e7d0b669372ae812b975328a79}
      \strng{authornamehash}{e816d9e7d0b669372ae812b975328a79}
