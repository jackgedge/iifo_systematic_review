@article{Hermes_2005,
  title = {{[Incorporation of multiple foreign bodies due to borderline personality disorder].}},
  author = {Hermes, D. and Schweiger, U. and Warnecke, K. K. and Tr{\"u}bger, D. and Hakim, S. G. and Sieg, P.},
  year = {2005},
