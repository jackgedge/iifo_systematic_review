@article{plasekEGSYSOESILRISK2010,
  title = {THE EGSYS AND OESIL RISK SCORES FOR CLASSIFICATION OF CARDIAC ETIOLOGY OF SYNCOPE: COMPARISON, REVALUATION, AND CLINICAL IMPLICATIONS},
  shorttitle = {THE EGSYS AND OESIL RISK SCORES FOR CLASSIFICATION OF CARDIAC ETIOLOGY OF SYNCOPE},
  author = {Plasek, Jiri and Doupal, Vlastimil and F{\"u}rstova, Jana and Martinek, Arnost},
  year = {2010},
  month = jun,
  journal = {Biomedical Papers},
  volume = {154},
  number = {2},
  pages = {169--173},
  issn = {12138118, 18047521},
  doi = {10.5507/bp.2010.026},
  urldate = {2023-12-11},
  abstract = {Methods. Retrospectively, 153 patients with syncope admitted to the Faculty Hospital in Ostrava in the year 2008 were enrolled for scoring with the EGSYS and OESIL risk scores. The computed scores were assessed with respect to the diagnosed etiology of the syncope. Results. For each score system, the hypothesis that there is no difference in the score between the cardiac and the non-cardiac group was rejected (all p-values below 0.0001). Strong correlation between the three score systems was shown (Spearman correlation coefficient: r = 0.71 p {$<$} 0.0001 for EGSYS-U and OESIL, r = 0.88 p {$<$} 0.0001 for EGSYS-U and EGSYS-M, r = 0.60 p {$<$} 0.0001 for EGSYS-M and OESIL). Sensitivity and specificity of the scores were computed and compared. Conclusions. All three score systems were found capable of classifying cardiac syncopes, reducing unnecessary hospital admissions and improving syncope risk evaluation. EGSYS-U was recommended for emergency syncope management because it was found to have both high sensitivity and high specificity.},
  langid = {english},
  keywords = {Cardiac syncope,Decision-making,EGSYS,Emergency department,msc,OESIL,Syncope,Syncope management unit},
  file = {/Users/jackgedge/Zotero/storage/Q2TMYL2E/Plasek et al. - 2010 - THE EGSYS AND OESIL RISK SCORES FOR CLASSIFICATION.pdf}
}

