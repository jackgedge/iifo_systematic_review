@article{garganiPersistentPulmonaryCongestion2015,
  title = {Persistent pulmonary congestion before discharge predicts rehospitalization in heart failure: a lung ultrasound study},
  shorttitle = {Persistent pulmonary congestion before discharge predicts rehospitalization in heart failure},
  author = {Gargani, Luna and Pang, P. S. and Frassi, F. and Miglioranza, M.H. and Dini, F. L. and Landi, P. and Picano, E.},
  year = {2015},
  month = dec,
  journal = {Cardiovascular Ultrasound},
  volume = {13},
  number = {1},
  pages = {40},
  issn = {1476-7120},
  doi = {10.1186/s12947-015-0033-4},
  urldate = {2023-12-11},
  abstract = {Background: B-lines evaluated by lung ultrasound (LUS) are the sonographic sign of pulmonary congestion, a major predictor of morbidity and mortality in patients with heart failure (HF). Our aim was to assess the prognostic value of B-lines at discharge to predict rehospitalization at 6 months in patients with acute HF (AHF). Methods: A prospective cohort of 100 patients admitted to a Cardiology Department for dyspnea and/or clinical suspicion of AHF were enrolled (mean age 70 {\textpm} 11 years). B-lines were evaluated at admission and before discharge. Subjects were followed-up for 6-months after discharge. Results: Mean B-lines at admission was 48 {\textpm} 48 with a statistically significant reduction before discharge (20 {\textpm} 23, p {$<$} .0001). During follow-up, 14 patients were rehospitalized for decompensated HF. The 6-month event-free survival was highest in patients with less B-lines ({$\leq$} 15) and lowest in patients with more B-lines ({$>$} 15) (log rank {$\chi$}2 20.5, p {$<$} .0001). On multivariable analysis, B-lines {$>$} 15 before discharge (hazard ratio [HR] 11.74; 95 \% confidence interval [CI] 1.30--106.16) was an independent predictor of events at 6 months. Conclusions: Persistent pulmonary congestion before discharge evaluated by ultrasound strongly predicts rehospitalization for HF at 6-months. Absence or a mild degree of B-lines identify a subgroup at extremely low risk to be readmitted for HF decompensation.},
  langid = {english},
  keywords = {b-lines,lung ultrasound,Lung ultrasoundB-linesUltrasound lung cometsPul,msc,prognosis,pulmonary congestion,ultrasound lung comets},
  file = {/Users/jackgedge/Zotero/storage/IXE5QF8Y/Gargani et al. - 2015 - Persistent pulmonary congestion before discharge p.pdf}
}

@article{gargiuloSelfharmingBehavioursAsylum2021,
  title = {Self-harming behaviours of asylum seekers and refugees in Europe: A systematic review},
  shorttitle = {Self-harming behaviours of asylum seekers and refugees in Europe},
