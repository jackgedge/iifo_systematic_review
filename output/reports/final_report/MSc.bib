  pages = {1087--1091},
  doi = {10.1097/00003246-199010000-00007},
  abstract = {Treatment plans for pediatric septic shock advocate increasing oxygen consumption ({\.V}O2). Recent studies in septic shock indicate that improving oxygen delivery ({\.D}O2) by increasing blood flow will increase {\.V}O2. We prospectively examined the effect on {\.V}O2 of improving {\.D}O2 by increasing oxygen content (CO2) with blood transfusion in eight hemodynamically stable septic shock patients. Transfusion consisted of 8 to 10 ml/kg of packed RBC over 1 to 2 h. Hemodynamic and oxygen transport measurements were obtained before and after blood transfusion. Transfusion significantly (p {$<$} .05) increased Hgb and Hct from 10.2 {\textpm} 0.8 g/dl and 30 {\textpm} 2\% to 13.2 {\textpm} 1.4 g/dl and 39 {\textpm} 4\%, respectively (mean {\textpm} SD). {\.D}O2 significantly (p {$<$} .05) increased after transfusion (599 {\textpm} 65 to 818 {\textpm} 189 ml/min{$\cdot$}m2), but {\.V}O2 did not change (166 {\textpm} 68 to 176 {\textpm} 74 ml/min{$\cdot$}m2; NS). In pediatric septic shock patients, increasing CO2 by blood transfusion may not increase {\.V}O2.},
  keywords = {msc},
  file = {/Users/jackgedge/Zotero/storage/DMFT6W85/mink1990.pdf.pdf}
}

