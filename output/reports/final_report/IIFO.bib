@article{Tavallaei_2021e,
  title = {Intentional Ingestion of a Metallic Wire Causing Perforation and Retroperitoneal Abscess: A Case Report},
  author = {Tavallaei, Mehdi and Bahadorinia, Mahsa and Haj Mohamad Ebrahim Ketabforoush, Arsh},
  year = {2021},
  journal = {Clinical Medicine Insights. Case Reports},
  doi = {10.1177/11795476211025919},
  abstract = {Foreign body ingestion is a frequent condition, with the majority of foreign bodies (FBs) tending to spontaneously proceed along the gastrointestinal tract  without any major complications. A wide range of procedures are available to  remove FBs; however, a real challenge exists in managing sharp, rigid, and long  foreign objects, which are related to higher rates of complications. A  34-year-old man who intentionally swallowed a metallic wire of 20\,cm length,  presented to our ED with abdominal pain 2\,weeks after the ingestion. The FB had  migrated to the stomach and duodenum. Complications included perforation of the  duodenum and ascending colon and a retroperitoneal abscess. FB removal was done  via laparotomy, followed by the repair of perforations and damaged tissues. This  case highlights the complications of a FB presence in the gastrointestinal tract  for 14\,days and emphasizes the importance of urgent and appropriate management of  such conditions.}
}

