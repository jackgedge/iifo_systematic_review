  author = {Trzeciak, Stephen and Dellinger, R. Phillip and Parrillo, Joseph E. and Guglielmi, Massimiliano and Bajaj, Jasmeet and Abate, Nicole L. and Arnold, Ryan C. and Colilla, Susan and Zanotti, Sergio and Hollenberg, Steven M.},
