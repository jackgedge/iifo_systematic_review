@article{McNutt_2001,
  title = {Bite the Bullet: Lead Poisoning after Ingestion of 206 Lead Bullets.},
  author = {McNutt, T. K. and {Chambers-Emerson}, J. and Dethlefsen, M. and Shah, R.},
  year = {2001},
