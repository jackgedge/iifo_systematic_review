@article{warnerPrevalenceSurvivorBias,
  title = {Prevalence of Survivor Bias in Observational Studies on Fresh Frozen Plasma:Erythrocyte Ratios in Trauma Requiring Massive Transfusion},
  author = {Warner, David S},
  abstract = {Observational studies on transfusion in trauma comparing high versus low plasma:erythrocyte ratio were prone to survivor bias because plasma administration typically started later than erythrocytes. Therefore, early deaths were categorized in the low plasma:erythrocyte group, whereas early survivors had a higher chance of receiving a higher ratio. When early deaths were excluded, however, a bias against higher ratio can be created. Survivor bias could be reduced by performing before-and-after studies or treating the plasma:erythrocyte ratio as a time-dependent covariate.},
