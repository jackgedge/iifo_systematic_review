  keywords = {BODIES,BODY INGESTION,COMPLICATIONS,Endoscopy,ENDOSCOPY,exclude,fav,Foreign body,GASTROINTESTINAL-TRACT,IIFO,Ingestion,INMATES,MANAGEMENT,msc,NEED,Prevention,Prisoner,read,Recurrence,RECURRENT,Swallower},
