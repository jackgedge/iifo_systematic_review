@article{corraltorresPrognosticValueVenous2020,
  title = {Prognostic value of venous blood analysis at the start of CPR in non-traumatic out-of-hospital cardiac arrest: association with ROSC and the neurological outcome},
  shorttitle = {Prognostic value of venous blood analysis at the start of CPR in non-traumatic out-of-hospital cardiac arrest},
  author = {Corral Torres, Ervigio and {Hern{\'a}ndez-Tejedor}, Alberto and Su{\'a}rez Bustamante, Rosa and De El{\'i}as Hern{\'a}ndez, Ram{\'o}n and Casado Fl{\'o}rez, Isabel and San Juan Linares, Antonio},
