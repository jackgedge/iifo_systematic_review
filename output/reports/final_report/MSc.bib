@article{tanEtiologySurgicalManagement2021,
  title = {Etiology and surgical management of pediatric acute colon perforation beyond the neonatal stage.},
  author = {Tan, Sarah Siyin and Wang, Kai and Pang, Wenbo and Wu, Dongyang and Peng, Chunhui and Wang, Zengmeng and Zhang, Dan and Chen, Yajun},
