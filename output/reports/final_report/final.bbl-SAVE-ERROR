      \strng{namehash}{36414d3770d73aa1ebee690a2047748f}
      \strng{fullhash}{36414d3770d73aa1ebee690a2047748f}
      \strng{fullhashraw}{36414d3770d73aa1ebee690a2047748f}
      \strng{bibnamehash}{36414d3770d73aa1ebee690a2047748f}
      \strng{authorbibnamehash}{36414d3770d73aa1ebee690a2047748f}
