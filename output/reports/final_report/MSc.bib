@article{cevizciIntestinalPerforationDue2012,
  title = {[Intestinal perforation due to multiple magnet ingestion: a case report]},
  shorttitle = {[Intestinal perforation due to multiple magnet ingestion},
  author = {Cevizci, Mehmet Nuri and Karada{\u g}, Cetin Ali and Demir, Mesut and Dokucu, Ali Ihsan},
