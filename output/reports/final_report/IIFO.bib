@article{Macmanus_1941,
  title = {Perforations of the Intestine by Ingested Foreign Bodies: {{Report}} of Two Cases and Review of the Literature},
  shorttitle = {Perforations of the Intestine by Ingested Foreign Bodies},
  author = {Macmanus, Joseph E.},
