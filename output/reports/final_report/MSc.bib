@article{shortCustodyVsCare2009,
  title = {Custody vs care: attitudes of prison staff to self-harm in women prisoners---a qualitative study},
  shorttitle = {Custody vs care},
  author = {Short, Vicky and Cooper, Jayne and Shaw, Jenny and Kenning, Cassandra and Abel, Kathryn and {Chew-Graham}, Carolyn},
  year = {2009},
  month = jun,
  journal = {The Journal of Forensic Psychiatry \& Psychology},
  volume = {20},
  number = {3},
  pages = {408--426},
  publisher = {Routledge},
  issn = {1478-9949},
  doi = {10.1080/14789940802377114},
  urldate = {2024-10-29},
  abstract = {Self-harm rates amongst the UK female prison population are disproportionately high. Prison staff potentially have a crucial role in the identification and management of female prisoners at risk; despite this there has been little focus on the attitudes of prison staff towards female prisoners who self-harm. This paper presents such an explanation; qualitative methods were used, with semi-structured interviews with eight prison officers and five healthcare staff from one female prison in the North of England. Interviews were tape recorded, transcribed verbatim, and analysed iteratively, until category saturation was achieved. The data suggests that prison staff labelled self-harm as either `genuine' or `non-genuine.' Women whose self-harm was perceived as non-genuine by staff were viewed as `rational manipulators,' self-harming to achieve particular ends. Staff described feelings of resentment towards these women. Most staff reported that balancing their welfare and security functions was difficult, feeling most confident with their custody role. They described feeling untrained and unsupported in their welfare role, and pressurised due to time constraints and reported low staffing levels. This combination of factors left most staff reporting lack of confidence in dealing with women who self-harm.},
  keywords = {msc,prison staff,prisoners,prisons,self-harm,women},
  file = {/Users/jackgedge/Zotero/storage/7SHDSCCD/Short et al. - 2009 - Custody vs care attitudes of prison staff to self.pdf}
}

@article{shujaLongitudinalAnalysisEpidemiology2016,
  title = {A longitudinal analysis of the epidemiology and economic impact of ER visits and inpatient admissions for alimentary foreign body ingestion in the united states},
  author = {Shuja, A. and De Melo, S. and Scolapio, J.},
  year = {2016},
  journal = {American Journal of Gastroenterology},
  volume = {111},
  number = {(Shuja A.; De Melo S.; Scolapio J.) University of Florida, College of Medicine, Jacksonville, FL, United States},
  pages = {S140},
  issn = {1572-0241},
  doi = {10.1038/ajg.2016.354},
  abstract = {Introduction: Foreign body ingestion occur commonly. The majority of ingested foreign bodies will pass spontaneously. However, studies have shown that in the setting of intentional ingestion, the rate of endoscopic intervention may be much higher (63\%- 76\%) and the need for surgical intervention ranges from 12\% to 16\%. The majority of foreign body ingestions occur in the pediatric population. In adults, foreign body ingestion (i.e. non-food objects) occurs more commonly in those with psychiatric disorders, developmental delay, alcohol intoxication, and in incarcerated individuals seeking secondary gain via release to a medical facility. Methods: The Nationwide Emergency Department Sample (NEDS) and National Inpatient Sample (NIS) database was used to determine the Emergency Department (ED) and inpatient burden of foreign body ingestion in the GI tract between 2006 and 2013. We analyzed all the patients with foreign body in the digestive system (ICD-9 code: 938), as their primary discharge diagnosis. Results: From 2006 to 2013, the number of patients presenting to the ED with foreign body ingestion in the alimentary tract increased considerably from 44,284 to 52,947. The rate of visits (per 100,000 visits) had increased from 14.8 to 16.7. The rate of admission amongst the patients presenting to the ED increased from 1\% to 1.5\%. The average length of stay (LoS) has increased from 2 to 3 days with associated significant increase in mean cost per discharge from \$4,957 to 24,314. The in-hospital mortality throughout these years remained negligible. Conclusion: Emergency department visits for foreign body ingestions have increased during the time frame analyzed. Not only the average LoS has increased, the cost associated with these hospitalizations has also increased almost five times. Increased ER presentations and hospitalizations could be due to increased incidence and prevalence of eosinophilic esophagitis, psychiatric illnesses, dietary non-compliance in patients at-risk for foreign body ingestion. We postulate that the increase in cost is likely attributed to more diagnostic and/or therapeutic interventions. More emphasis should be placed on interventions like dietary, nutrition and psychiatric counselling and closer monitoring of incarcerated individuals, to prevent further ER presentations and hospitalizations. In order to deliver more cost-conscious care, further studies to determine optimal methods to avoid these events from occuring.},
  langid = {english},
  keywords = {adult,alcohol intoxication,child,clinical study,counseling,data base,developmental disorder,diagnosis,emergency ward,eosinophilic esophagitis,foreign body,gastrointestinal tract,hospital mortality,hospital patient,hospitalization,human,ICD-9,ingestion,length of stay,mental disease,monitoring,msc,nutrition,prevalence,United States}
}

