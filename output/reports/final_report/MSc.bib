@article{rubertssonMechanicalChestCompressions2014,
  title = {Mechanical Chest Compressions and Simultaneous Defibrillation vs Conventional Cardiopulmonary Resuscitation in Out-of-Hospital Cardiac Arrest: The LINC Randomized Trial},
  shorttitle = {Mechanical Chest Compressions and Simultaneous Defibrillation vs Conventional Cardiopulmonary Resuscitation in Out-of-Hospital Cardiac Arrest},
  author = {Rubertsson, Sten and Lindgren, Erik and Smekal, David and {\"O}stlund, Ollie and Silfverstolpe, Johan and Lichtveld, Robert A. and Boomars, Rene and Ahlstedt, Bj{\"o}rn and Skoog, Gunnar and Kastberg, Robert and Halliwell, David and Box, Martyn and Herlitz, Johan and Karlsten, Rolf},
  year = {2014},
