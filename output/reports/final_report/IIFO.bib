@article{Tustumi_2020e,
  title = {Unusual Cause of Appendicitis. a Case Report of Acute Appendicitis Caused by Needle Ingestion},
  author = {Tustumi, Francisco and Hudari, Guilherme Garcia and Modolo, Natalia Rebeque and Morrell, Andr{\'e} Luiz Gioia and {de Miranda Neto}, Ant{\^o}nio Afonso and Dias, Andr{\'e} Roncon},
  year = {2020},
  journal = {International Journal Of Surgery Case Reports},
  doi = {10.1016/j.ijscr.2020.05.094},
  abstract = {INTRODUCTION: Appendicitis is an acute inflammatory process. The purpose of this report is to describe an unusual case of acute appendicitis in adult caused by  accidental ingestion of a needle, and treated with appendectomy. PRESENTATION OF  CASE: A 64 year-old white woman, seamstress, was admitted with abdominal pain  complaints. Plain radiograph and CT scan showed metal density suggesting a  foreign body in the lower right abdomen. A diagnostic laparoscopic procedure  identified a perforated appendicitis due to a needle perforation. Laparoscopic  appendicectomy and foreign body removal were performed. DISCUSSION: Acute  appendicitis due foreign body ingestion is extremely rare. Laparoscopy was the  chosen access once it was more suitable for inventory of the gastrointestinal  tract and help avoiding harm of the medical team during hand maneuvers of open  access surgery. CONCLUSION: Foreign body ingestion should be remembered as an  unusual differential diagnosis of acute appendicitis.}
}

@article{Udemgba_2021e,
  title = {A Case Report of an Unusual Left Atrial Mass},
  author = {Udemgba, Chinelo and Missov, Emil and Percy, Robert and Sattiraju, Srinivasan},
  year = {2021},
  journal = {European Heart Journal. Case Reports},
  doi = {10.1093/ehjcr/ytaa500},
  abstract = {BACKGROUND: Cardiac foreign bodies (FBs) are rare findings that may present as cardiac masses initially. Here, we present an exceptional and rare case of a  hypodermic needle FB that transmigrated to the left atrium and presented as a  left atrial mass. CASE SUMMARY: A 28-year-old woman with multiple psychiatric  disorders including intentional FB ingestion and self-inflicting injuries  presented to the emergency room with abdominal pain, nausea, vomiting, diarrhoea,  and chest pain that radiated to the left arm and face for 2\,weeks. An  echocardiogram was performed revealing a left atrial mass concerning for myxoma.  During the surgical removal of the mass, a hypodermic needle was found attached  to the roof of the left atrium surrounded by thrombotic and fibrotic tissue,  which was confirmed by pathology. DISCUSSION: Cardiac FBs are caused generally by  penetrating wounds from direct trauma. Fewer cases have been reported regarding  cardiac FB caused by ingestion from migration of the object to the heart. Signs  and symptoms for cardiac FB may mimic those of cardiac masses. Cardiac FBs often  require surgical intervention to avoid complications.}
}

