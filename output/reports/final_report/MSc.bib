@article{bloomAgreementAbnormalVenous2014,
  title = {The agreement between abnormal venous lactate and arterial lactate in the ED: a retrospective chart review},
  shorttitle = {The agreement between abnormal venous lactate and arterial lactate in the ED},
  author = {Bloom, B. and Pott, J. and Freund, Y. and Grundlingh, J. and Harris, T.},
