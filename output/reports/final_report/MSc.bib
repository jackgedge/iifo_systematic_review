@article{russellExtractionEsophagealForeign2014,
  title = {Extraction of esophageal foreign bodies in children: rigid versus flexible endoscopy.},
  author = {Russell, Robert and Lucas, Alan and Johnson, Joffre and Yannam, Govarhana and Griffin, Russell and Beierle, Elizabeth and Anderson, Scott and Chen, Mike and Harmon, Carroll},
  year = {2014},
