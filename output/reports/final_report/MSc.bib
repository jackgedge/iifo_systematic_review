@inproceedings{khalidAidingParamedicsRecognition2015,
  title = {Aiding paramedics recognition of type two respiratory failure (T2RF)},
  author = {Khalid, Tanveer and Baksi, Saumitra},
  year = {2015},
  month = sep,
  pages = {PA2533-PA2533},
  doi = {10.1183/13993003.congress-2015.pa2533},
  abstract = {Introduction: High flow oxygen administration increases morbidity/mortality in patients susceptible to T2RF. Ambulances need aids to recognise these patients and aim for target saturations using controlled oxygen. The British Thoracic Society recommends identifying patients with T2RF using tools like an electronic oxygen alert. The ambulance service for Royal Blackburn Hospital (RBH) does not have an electronic alert system. The aim of this audit is to compare paramedics' recognition of patients with known T2RF before and after creating an electronic oxygen alert. Method: This prospective audit from January-April 2014 at RBH included inpatients with known T2RF who received high flow oxygen in the ambulance, as documented on the ambulance sheet and first arterial blood gas. On discharge, an electronic oxygen alert was set up for each patient. We assessed if ambulance records noted the patient as having previous T2RF on readmission. Results: 27 patients met inclusion criteria. The data before the creation of an electronic alert showed ambulances identified 41\% as having COPD or T2RF. Of these 27 patients, 70\% received uncontrolled high flow oxygen in the ambulance and only 4\% had documented target saturations on the ambulance sheet. NIV was required for 67\% with a mortality of 22\% in this group. Of the 27 patients, 22\% were readmitted and noted to have known T2RF by paramedics via the electronic alert. Half of those readmitted received uncontrolled oxygen. Target saturations for 83\% were not recorded. Conclusion: Identifying T2RF patients by ambulances improved with using electronic oxygen alerts, however additional training for paramedics on oxygen administration in such patients is required.},
  keywords = {msc}
}

@article{khanAssociationVolumeFluid2020,
  title = {Association Between Volume of Fluid Resuscitation and Intubation in High-Risk Patients With Sepsis, Heart Failure, End-Stage Renal Disease, and Cirrhosis},
  author = {Khan, Rizwan A. and Khan, Nauman A. and Bauer, Seth R. and Li, Manshi and Duggal, Abhijit and Wang, Xiaofeng and Reddy, Anita J.},
  year = {2020},
  month = feb,
  journal = {Chest},
  volume = {157},
  number = {2},
  pages = {286--292},
  issn = {00123692},
  doi = {10.1016/j.chest.2019.09.029},
  urldate = {2023-12-11},
  abstract = {BACKGROUND: Initial fluid resuscitation volume for sepsis is controversial, particularly in patients at high baseline risk for complications. This study was designed to assess the association between 30 mL/kg crystalloids and intubation in patients with sepsis or septic shock and heart failure, end-stage renal disease, or cirrhosis. METHODS: This propensity score-matched retrospective cohort study included patients with sepsis or septic shock admitted to a large medical ICU. Primary exposure was IV fluid volume in the first 6 h following sepsis diagnosis, divided into two cohorts: \$ 30 mL/kg (standard group) and {$<$} 30 mL/kg (restricted group). The primary outcome was need for mechanical ventilation within 72 h following initiation of fluid resuscitation. Secondary outcomes were length of stay, ventilator days, and time to intubation. RESULTS: A total of 208 patients were included, with 104 (50\%) in the restricted group ({$<$} 30 mL/kg) and 104 in the standard group (\$ 30 mL/kg). No difference in intubation incidence was detected between the two groups, with 36 patients (35\%) in the restricted group and 33 (32\%) in the standard group (adjusted OR, 0.75; 95\% CI, 0.41-1.36; P {$\frac{1}{4}$} .34) intubated. There was no difference between standard and restricted groups in alive ICU-free days (17 {\AE} 11 days vs 17 {\AE} 10 days; P {$\frac{1}{4}$} .64), duration of mechanical ventilation (10 {\AE} 12 days vs 11 {\AE} 16 days; P {$\frac{1}{4}$} .96), or hours to intubation (16 {\AE} 19 h vs 14 {\AE} 15; P {$\frac{1}{4}$} .55). CONCLUSIONS: No differences were detected in the incidence of intubation in patients with sepsis and cirrhosis, end-stage renal disease, or heart failure who received guidelinerecommended fluid resuscitation with 30 mL/kg compared with patients initially resuscitated with a lower fluid volume.},
  langid = {english},
  keywords = {cirrhosis,heart failure,mechanical ventilation,msc,renal failure,septic shock},
  file = {/Users/jackgedge/Zotero/storage/7MJQPQXB/Khan et al. - 2020 - Association Between Volume of Fluid Resuscitation .pdf;/Users/jackgedge/Zotero/storage/TYCT8FK7/khan2019.pdf.pdf}
}

