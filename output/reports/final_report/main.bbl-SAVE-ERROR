    \entry{Sundvall_2015}{article}{}{}
      \name{author}{5}{}{%
        {{hash=d948762d57f59bb592f1a297293d6d45}{%
           family={Sundvall},
           familyi={S\bibinitperiod},
           given={Maria},
           giveni={M\bibinitperiod}}}%
        {{hash=d6d0afbfc10d270614e41dab5bf7cf4d}{%
           family={Tidemalm},
